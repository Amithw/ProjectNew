                            </div>
                            <div class="panel-body">
                                <div class="row1">
                                    <div class="col-lg-9">
   

    <% practices.forEach(function (organization) { %>
    <br>



    <%= organization.student_id %><br>

    <% if(organization.name){  %><br>
    <%= organization.name %><br>
    <%  } %>
    <% if(organization.job_title){  %>
        <%= organization.job_title %>
        <%  } %><br>
    <% if(organization.phone_number){  %>
    <%= organization.phone_number %>
    <%  } %><br>
    <% if(organization.email){  %>
    <%= organization.email %>
    <%  } %><br>
    <% if(organization.vacancy){  %>
    <%= organization.vacancy %>
    <%  } %><br>
    <% if(organization.closing_date){  %>
    <%= organization.closing_date %>
    <%  } %><br>
    <% if(organization.description){  %>
    <%= organization.description %>
    <%  } %><br>
   
    <% if(organization.web){  %>
    <a href="https://<%= organization.web %>">View WebSite</a>
    <%  } %><br>

    <a href="/student_send_cv/<%= organization.student_id %>">Apply</a>
