        <script>

            var token = localStorage.getItem("token");
            var id;
            var web;
            if (!!token) {
                $.ajax({
                    type: "GET",
                    url: `/users/${token}`,
                    dataType: "json",
                    success: function (data) {

                        $("#org_id").text(data.student_id);
                        $("#name").text(data.name);
                        $("#web").text(data.web);
                        $("#phone_number").text(data.phone_number);
                        $("#email").text(data.email);
                        $("#description").text(data.description);

                        id = data.student_id;
                        web = data.web;
                        console.log(id);

                        document.getElementById("cart").href = './organizations/viewCart/' + id;
                        document.getElementById("view").href = './organizations/viewRequest/' + id;
                        document.getElementById("web").href = 'http://' + web;
                    },
                    error: function () {
                        console.log("error from the method");
                    }
                });

            } else {
                window.location.replace("/");
            }

            $("#logout").click(function () {
                localStorage.clear("token");
                window.location.replace("login");
            });



        </script>
